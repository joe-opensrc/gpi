; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

#godot.projectconfig_version=5

[application]

config/name="Snu"
run/main_scene="res://GameEntryPoint.tscn"
config/features=PackedStringArray("4.3")
config/icon="res://icon.svg"

[autoload]

Enjyn="*res://autoloads/Enjyn/Enjyn.tscn"
Director="*res://autoloads/Director/Director.tscn"

[display]

window/size/viewport_height=720
window/stretch/scale_mode="integer"
#commonwindow/size/viewport_width=1280
#window/size/mode=4
#exclusive-fullscreen#vector#window/stretch/mode="canvas_items"
#window/stretch/aspect="expand"
#pixelwindow/stretch/mode="viewport"
#window/size/borderless=true
#window/size/window_width_override=640
#window/size/window_height_override=360

[editor]

naming/scene_name_casing=1
#PascalCasenaming/script_name_casing=1
#PascalCase#naming/node_name_casing=1
"#camelCase[input]req_quit"={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":true,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":81,"key_label":0,"unicode":113,"location":0,"echo":false,"script":null)
]
}

[input]

user_req_quit={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":true,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":81,"key_label":0,"unicode":113,"location":0,"echo":false,"script":null)
]
}

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_vertices_to_pixel=true
#commontextures/lossless_compression/force_png=true
#pixeltextures/default_filters/use_nearest_mipmap_filter=true
#nearesttextures/canvas_textures/default_texture_repeat=1
#enable2d/snap/snap_2d_transforms_to_pixel=true
#vector#features:#renderer/rendering_method="gl_compatibility"
#renderer/rendering_method.mobile="gl_compatibility"
